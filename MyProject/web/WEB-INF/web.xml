<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServletSignUp</servlet-name>
        <servlet-class>NewServletSignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>role_authenticate</servlet-name>
        <servlet-class>role_authenticate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_admin</servlet-name>
        <servlet-class>Servlet_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PasswordRecoverServlet</servlet-name>
        <servlet-class>PasswordRecoverServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadImage</servlet-name>
        <servlet-class>UploadImage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServletSignUp</servlet-name>
        <url-pattern>/NewServletSignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>role_authenticate</servlet-name>
        <url-pattern>/role_authenticate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_admin</servlet-name>
        <url-pattern>/Servlet_admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PasswordRecoverServlet</servlet-name>
        <url-pattern>/PasswordRecoverServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadImage</servlet-name>
        <url-pattern>/UploadImage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
